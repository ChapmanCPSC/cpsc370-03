<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chapman.yourmoney"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity 
            android:name="com.chapman.yourmoney.activities.MainMenu"
            android:label="@string/app_name">
            <!-- <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter> -->
        </activity>
        <activity android:name="com.chapman.yourmoney.activities.Startup"
    		android:label="@string/app_name"
    		android:theme="@style/Startup">
    		<intent-filter>
        		<action android:name="android.intent.action.MAIN" />
        		<category android:name="android.intent.category.LAUNCHER" />
    		</intent-filter>
		</activity>
		<activity
		    android:name="com.chapman.yourmoney.activities.SelectTransaction"
		    android:label="@string/app_name" />
		<activity android:name="com.chapman.yourmoney.activities.AddEditTransaction"
		    android:label="@string/app_name"
		    android:theme="@style/Theme.SelectDate">
    	</activity>
    </application>

</manifest>
